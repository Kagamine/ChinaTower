<div class="content-wrap">
    <div class="container">
        <form action="/setting" method="post">
            <h2>参数设定</h2>
            <h3>整合分析</h3>
            <hr />
            <p>
                郊区半径(米)
                <br />
                <input type="text" name="suburb" value="<%= settings.share.suburb %>" />
            </p>
            <p>
                密集城区半径(米)
                <br />
                <input type="text" name="crowded" value="<%= settings.share.crowded %>" />
            </p>
            <p>
                农村半径(米)
                <br />
                <input type="text" name="village" value="<%= settings.share.village %>" />
            </p>
            <p>
                一般城区半径(米)
                <br />
                <input type="text" name="city" value="<%= settings.share.city %>" />
            </p>
            <h3>新建分析</h3>
            <hr />
            <p>
                郊区半径(米)
                <br />
                <input type="text" name="_suburb" value="<%= settings.plan.suburb %>" />
            </p>
            <p>
                密集城区半径(米)
                <br />
                <input type="text" name="_crowded" value="<%= settings.plan.crowded %>" />
            </p>
            <p>
                农村半径(米)
                <br />
                <input type="text" name="_village" value="<%= settings.plan.village %>" />
            </p>
            <p>
                一般城区半径(米)
                <br />
                <input type="text" name="_city" value="<%= settings.plan.city %>" />
            </p>
            <p><input type="submit" class="btn btn-primary" value="保存"></p>
        </form>
    </div>
</div>