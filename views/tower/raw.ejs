<% towers.forEach(x => { %>
<tr data-tower="<%= x._id %>" data-status="<%= x.status %>" data-url="<%= x.url %>" data-scene="<%= x.scene %>" data-provider="<%= x.provider %>" data-lon="<%= x.lon %>" data-lat="<%= x.lat%>" data-picture="<%= x.picture %>" data-height="<%= x.height %>">
    <td><input type="checkbox" data-id="<%= x._id %>" class="tower-chk" /></td>
    <td class="name"><%= x.name %></td>
    <td class="city"><%= x.city %></td>
    <td class="district"><%= x.district %></td>
    <td class="address"><%= x.address %></td>
    <td><%= x.providerDisplay %></td>
    <td><%= x.scene %></td>
    <td class="type"><%= x.type %></td>
    <td><%= x.height %> 米</td>
    <td><%= x.status %></td>
    <td>
        <a class="btn btn-small btn-info" href="javascript:editTower('<%= x._id %>')">编辑</a>
        <a class="btn btn-small btn-danger" href="javascript:deleteTower('<%= x._id %>')">删除</a>
    </td>
</tr>
<% }) %>